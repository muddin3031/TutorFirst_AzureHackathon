  <body>
<div class="container-fluid">
  <div class="row">
    <nav id="sidebarMenu" class="col-md-3 col-lg-2 d-md-block bg-light sidebar collapse" style="padding-right:10px;">
      <div class="sidebar-sticky pt-3">
          <div class="container-fluid" style="padding-right: 0;">
              <h2>TutorFirst</h2>

          </div>
        <ul class="nav flex-column">
          <li class="nav-item">
            <a class="nav-link active" href="#">
              <span data-feather="home"></span>
              <span class="sr-only">(current)</span>
            </a>
          </li>
          <li class="nav-item">
            <a class="nav-link" href="#" onclick="overview();">
              <span data-feather="file"></span>
               Overview
            </a>
          </li>
          <li class="nav-item">
            <a class="nav-link" href="#" onclick="appointments();">
              <span data-feather="file"></span>
              My Appointments
            </a>
          </li>
          <li class="nav-item">
            <a class="nav-link" href="#" onclick="assignments();">
              <span data-feather="shopping-cart"></span>
              Assignments
            </a>
          </li>
          <li class="nav-item">
            <a class="nav-link" href="#" onclick="grades();">
              <span data-feather="users"></span>
              Grades
            </a>
          </li>    
        </ul>
      </div>
    </nav>

    <main role="main" class="col-md-9 ml-sm-auto col-lg-10 px-md-4">
<div id="overview">
  <div class="d-flex justify-content-between flex-wrap flex-md-nowrap align-items-center pt-3 pb-2 mb-3 border-bottom">
    <h1 class="h2">Welcome, <%=studentObj.fName %>!</h1>
  </div>
  <h1>Overview</h1>
</div>
        
<div id="appointments">
      <div class="d-flex justify-content-between flex-wrap flex-md-nowrap align-items-center pt-3 pb-2 mb-3 border-bottom">
        <h1 class="h2">Appointments</h1>
      </div>

      <div class="btn-toolbar mb-2 mb-md-0">
        <h1 style="padding-bottom:10px ;">Schedule an Appointment</h1>
        <form action="/schedule" method="POST">
          <label for="apptDate">Choose a date:</label>
          <input type="date" name="apptDate" class="btn btn-sm btn-outline-secondary">
          <label for="subject" style="padding-left:10px ;">Choose subject:</label>
             <select class="btn btn-sm btn-outline-secondary" id="subjects" name="subjects">
               <option value="English">English</option>
               <option value="Math">Math</option>
               <option value="Biology">Biology</option>
               <option value="Chemistry">Chemistry</option>
               <option value="Physics">Physics</option>
             </select>
             <label for="" style="margin-left: 10px;">Choose a time:</label>
             <input class="btn btn-sm btn-outline-secondary" type="time" name="time">
             <% if (message.length > 0) { %>
              <div class="alert alert-danger">
                <%= message %>
              </div>
             <% } %>
             <div class="btn-group mr-2">
              <button type="submit" class="btn btn-sm btn-outline-secondary" style="margin-left: 10px;">Schedule</button>
            </div>
         </form>
      
      </div>
      <canvas class="my-4 w-100" id="myChart" width="900" height="380"></canvas>
      <h2>Section title</h2>
      <div class="table-responsive">
        <table class="table table-striped table-sm">
          <thead>
            <tr>
              <th>Date</th>
              <th>Tutor Name</th>
              <th>Appointment Time</th>
              <th>Zoom Link</th>
            </tr>
          </thead>
          <tbody>
            <% for (var i = 0; i < studentObj.appts.length; i=i+3) { %>
              <tr> 
              <td><%= studentObj.appts[i] %></td>
              <td><%= studentObj.appts[i+1] %></td>
              <td><a href="/room?room_id=<%= studentObj.appts[i+2] %>" target="_blank">Room Link</a></td>
            </tr>
            <% } %>
          </tbody>
        </table>
      </div>
    </div>
<div id="assignments">
    <h1>Assignments</h1> 
</div>
<div id="grades">
    <h1>Grades</h1>
</div>
    </main>
  </div>
</div>
<script>
    overview();
    
    function overview() {
        var overViewDiv = document.getElementById("overview")
        var apptDiv = document.getElementById("appointments")
        var assignment = document.getElementById("assignments")
        var grades = document.getElementById("grades")
        overViewDiv.style.display = "block"
        apptDiv.style.display = "none"
        assignment.style.display = "none"
        grades.style.display = "none"
    }

    function appointments(){
        var overViewDiv = document.getElementById("overview")
        var apptDiv = document.getElementById("appointments")
        var assignment = document.getElementById("assignments")
        var grades = document.getElementById("grades")
        apptDiv.style.display = "block"
        overViewDiv.style.display = "none"
        assignment.style.display = "none"
        grades.style.display = "none"
        
    }
    
    function assignments(){
        var overViewDiv = document.getElementById("overview")
        var assignDiv = document.getElementById("assignments")
        console.log(assignDiv);
        var apptDiv = document.getElementById("appointments")
        var grades = document.getElementById("grades")
        overViewDiv.style.display = "none"
        assignDiv.style.display = "block"
        apptDiv.style.display = "none"
        grades.style.display = "none"
    }

    function grades() {
        var overViewDiv = document.getElementById("overview")
        var apptDiv = document.getElementById("appointments")
        var assignment = document.getElementById("assignments")
        var grades = document.getElementById("grades")
        apptDiv.style.display = "none"
        overViewDiv.style.display = "none"
        assignment.style.display = "none"
        grades.style.display = "block"
    }
</script>
